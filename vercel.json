{
  "redirects": [
    
    { "source": "/sitemap.xml", "destination": "https://duncan.dev/sitemap.xml"},
    { "source": "/blog/atom(.*)", "destination": "https://duncan.dev/feed.xml"},
    { "source": "/rss", "destination": "https://duncan.dev/feed.xml"},
    { "source": "/atom(.*)", "destination": "https://duncan.dev/feed.xml"},
    { "source": "/(.*).rss", "destination": "https://duncan.dev/feed.xml"},
    { "source": "/(.*).xml", "destination": "https://duncan.dev/feed.xml"},
    { "source": "/(.*)", "destination": "https://duncan.dev/$1" }
  ]
}
